{
  "name": "royal-visit-bot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "format": "rome format --write src",
    "lint": "rome check src",
    "lint:fix": "rome check --apply-suggested src",
    "validate": "npm run lint && npm run test && npm run build",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.185",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "rome": "^0.9.2-next",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.2"
  }
}
