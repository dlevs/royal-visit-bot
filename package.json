{
  "name": "royal-visit-bot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "format": "rome format --write src",
    "lint": "rome check src",
    "lint:fix": "rome check --apply-suggested src",
    "validate": "npm run lint && npm run test && tsc -noEmit",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/utilities": "^3.2.0",
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.2",
    "@types/lodash": "^4.14.185",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "rome": "^0.9.2-next",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  }
}
